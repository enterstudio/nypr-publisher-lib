{{#if isTwitter}}
  <a href="https://twitter.com/{{item.contact-string}}" data-size="large" class="twitter-follow-button" data-show-count="false">Follow @{{item.contact-string}}</a>
{{else}}
  <a class="btn btn--circle btn--blacktext" href={{url}} target="_blank">
    <i class="fa fa-{{iconname}}"></i>
  </a>
{{/if}}
